{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "cwd": "babelrc",
            "root": ["./"],
            "extensions": [".js", ".ios.js", ".android.js"]
          }
        ]
      ]
    }
  }
}