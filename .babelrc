{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "reducers": "./src/reducers",
            "components": "./src/components",
            "containers": "./src/containers",
            "actions": "./src/actions",
            "screens": "./src/screens",
            "navigators": "./src/navigators"
          }
        }]
      ]
    }
  }
}
